  "main": "dist/start-server.js",
  "scripts": {
    "clear": "rimraf dist",
    "prestart": "npm run build",
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node ./dist/start-server.js --watch",
    "start:local": "npm run clear && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/start-server.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kozachokx/exercise-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kozachokx/exercise-tracker/issues"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  },
  "homepage": "https://github.com/Kozachokx/exercise-tracker#readme",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@prisma/client": "^4.13.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "mongodb": "^5.3.0",
    "prisma": "^4.13.0",
    "prisma-joi-generator": "^0.2.0"
  }
}
